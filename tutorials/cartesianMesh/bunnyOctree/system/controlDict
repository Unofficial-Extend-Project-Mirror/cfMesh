FoamFile
{
    version   2.0;
    format    ascii;
    class     dictionary;
    object    controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         true;  // Optional : no default application to run

startFrom           latestTime;

startTime           0;

stopAt              endTime;

endTime             10;

deltaT              1;

writeControl        timeStep;

writeInterval       20;

writeFormat         ascii;

runTimeModifiable   yes;

