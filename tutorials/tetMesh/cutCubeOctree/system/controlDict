FoamFile
{
    version   2.0;
    format    ascii;
    class     dictionary;
    object    controlDict;
}
// ************************************************************************* //

application         tetMesh;

startFrom           latestTime;

startTime           0;

stopAt              endTime;

endTime             500;

deltaT              1;

writeControl        timeStep;

writeInterval       100;

writeFormat         ascii;

writeCompression    on;

runTimeModifiable   yes;

